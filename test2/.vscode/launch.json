{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "args": [
                "--input-path",
                "tests\\inputs\\input_path",
                "--output",
                "tests\\outputs\\output",
                "--str-param",
                "some string"
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "program": "${file}"
        }
    ]
}